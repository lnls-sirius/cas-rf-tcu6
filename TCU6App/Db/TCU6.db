record(stringin, "$(P)$(R)Data"){
    field(DTYP, "stream")
    field(INP,  "@TCU6.proto getData($(P),$(R)) $(PORT) $(A)")
    field(SCAN, "1 second")
}

record(ai, "$(P)$(R)Tin-Mon"){
    field(DTYP, "Soft Channel")
    field(DESC, "Water inlet temp")
    field(PREC, "2")
    field(EGU,  "C")
}

